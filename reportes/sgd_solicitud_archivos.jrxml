	<queryString>
		<![CDATA[select d.orderindex, o.creation, o.numero, d.archivoid, d.barcode, u.nombre as usuario, s.nombre as sector, pp.codigo as precinto, institucion.nombre as institucion
 from solicitud o
JOIN solicituddetalle d on (o.id = d.solicitud_id)
JOIN usuario u on (o.usuario_id = u.id)
JOIN sector s ON (o.sector_id = s.id)
JOIN $P!{TABLA} a ON (d.archivoid = a.id)
JOIN $P!{TABLA}precinto pp ON (a.id = pp.$P!{TABLA}_id)
JOIN institucion ON (institucion.id = a.institucion_id)
WHERE o.id = $P{ID}]]>
	</queryString>
